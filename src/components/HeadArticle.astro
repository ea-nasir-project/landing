---
import BaseHead from "./BaseHead.astro";

interface Props {
  title: string;
  description: string;
  image?: ImageMetadata;
  publishedTime: string;
  author: string;
}

const { title, description, image, publishedTime, author } = Astro.props;
---

<BaseHead
  title={title}
  description={description}
  image={image}
  ogType="article"
  publishedTime={publishedTime}
  author={author}
  showRSS={true}
/>
