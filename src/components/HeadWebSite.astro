---
import BaseHead from "./BaseHead.astro";
import Schema from "./seo/Schema.astro";

interface Props {
  title: string;
  description: string;
  image?: ImageMetadata;
}

const { title, description, image } = Astro.props;
---

<BaseHead
  title={title}
  description={description}
  image={image}
  ogType="website"
  showRSS={false}
/>
<Schema type="organization" />
<Schema type="website" />
