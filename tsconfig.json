{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@components/*": [
        "src/components/*"
      ],
      "@layouts/*": [
        "src/layouts/*"
      ],
      "@images/*": [
        "src/assets/images/*"
      ],
      "@pages/*": [
        "src/pages/*"
      ],
      "@scripts/*": [
        "src/assets/scripts/*"
      ],
      "@styles/*": [
        "src/assets/styles/*"
      ]
    }
  }
}