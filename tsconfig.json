{
  "extends": "astro/tsconfigs/strict",
  "include": [
    ".astro/types.d.ts",
    "**/*"
  ],
  "exclude": [
    "dist"
  ],
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/images/*": [
        "src/assets/images/*"
      ],
      "@/icons/*": [
        "src/assets/icons/*"
      ],
      "@/scripts/*": [
        "src/assets/scripts/*"
      ]
    }
  }
}